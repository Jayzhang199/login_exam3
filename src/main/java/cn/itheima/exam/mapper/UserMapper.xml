<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.itheima.exam.mapper.UserMapper">
    <select id="login" parameterType="cn.itheima.exam.domain.User" resultType="cn.itheima.exam.domain.User">
        SELECT * FROM s_user WHERE username=#{username} AND  password=#{password}
    </select>



</mapper>